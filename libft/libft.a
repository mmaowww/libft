#include 'libft.h'
//#include <stdio.h>

int	ft_isalnum(int c)
{
	if ((c >= 'A' && c <= 'Z') || (c >= 'a' && c <= 'z')
		|| (c >= '1' && c <= '9'))
		return (1);
	return (0);
}

